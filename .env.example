# Portfolio RAG System Environment Variables
# Copy this file to .env and fill in your actual API keys

# LLM API Keys (you need at least one for RAG to work)
GROQ_API_KEY=your_groq_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration (usually don not need to change these for development)
DATABASE_URL=postgresql://portfolio:portfolio@database:5432/portfolio
VECTOR_DB_URL=postgresql://portfolio:portfolio@database:5432/portfolio

# RAG Configuration
RAG_ENABLED=true

# Frontend Configuration
FRONTEND_URL=http://localhost:3000
VITE_API_URL=http://localhost:8000

# Development Settings
DEBUG=true
